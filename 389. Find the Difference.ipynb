   "source": [
    "def findTheDifference(s,t):\n",
    "        s1= sorted(s)\n",
    "        t1 = sorted(t)\n",
    "        \n",
    "        for i in range(len(s1)):\n",
    "            if s1[i] != t1[i]:\n",
    "                return t1[i]\n",
    "        return t1[-1]\n"
   ]
